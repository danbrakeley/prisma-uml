{
  "name": "prisma-uml",
  "version": "1.1.1",
  "description": "A CLI to transform a Prisma schema to a PlantUML Entity RelationShip Diagram",
  "main": "./dist/prisma-uml.js",
  "bin": {
    "prisma-uml": "./dist/prisma-uml.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:js check:types",
    "build:js": "TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --mode=production",
    "check:types": "tsc -p tsconfig.prod.json --noEmit",
    "local": "yarn build && yarn link && chmod +x ./dist/prisma-uml.js",
    "semantic-release": "semantic-release",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepare": "husky install"
  },
  "release": {
    "branches": [
      "master",
      "next",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "keywords": [
    "UML",
    "prisma",
    "ERD",
    "Entity Relationship Diagram",
    "plantUML",
    "Unified Modeling Language"
  ],
  "repository": "emyann/prisma-uml",
  "homepage": "https://github.com/emyann/prisma-uml",
  "author": "Yann Renaudin",
  "license": "MIT",
  "dependencies": {
    "@prisma/internals": "^5.5.2",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "ts-toolbelt": "^9.6.0",
    "uuid": "^9.0.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/uuid": "^9.0.6",
    "@types/webpack": "^5.28.4",
    "@types/yargs": "^15.0.4",
    "babel-loader": "^9.1.3",
    "husky": ">=8.0.3",
    "jest": "^29.7.0",
    "lint-staged": ">=15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "snyk": "^1.1237.0",
    "source-map-loader": "^4.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.3.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "snyk": true,
  "lint-staged": {
    "*.{ts,js,css,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
